@import navigation-bar-mixin

.navigation-bar
  navigation-bar-base();
  padding-left var-padding--navigation-bar
  padding-right var-padding--navigation-bar

.navigation-bar__item
  navigation-bar__item()
  height var-height
  line-height var-line-height
  margin var-margin

.navigation-bar__title
  navigation-bar__title()
  margin var-margin

